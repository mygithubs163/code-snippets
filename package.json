{
  "name": "code-snippets",
  "displayName": "code-snippets",
  "publisher": "vscodepluginerchengzi",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "commands": [
      {
        "command": "code-snippets",
        "title": "Code Snippets Template"
      }
    ],
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/snippets.code-snippets"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/snippets.code-snippets"
      },
      {
        "language": "typescript",
        "path": "./snippets/snippets.code-snippets"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/snippets.code-snippets"
      }
    ],
    "keybindings": [
      {
        "command": "code-snippets",
        "key": "ctrl+f10",
        "mac": "cmd+f10",
        "when": "editorFocus"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "code-snippets",
          "group": "navigation", 
          "when": "explorerResourceIsFolder" 
        }
      ]
    }
  }
}
